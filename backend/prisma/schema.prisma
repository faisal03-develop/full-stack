generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id    String     @id @default(cuid())
  username   String @unique
  password   String
  email String  @unique
  role  String  @default("patient")
  createdAt DateTime @default(now())
  updatedAt DateTime? @updatedAt
}
